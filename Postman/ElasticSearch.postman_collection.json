					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"let id = pm.environment.get(\"id\");",
									" id++",
									"pm.environment.set(\"id\",id)",
									"",
									"let name = \"Prodcut \" + id",
									"pm.environment.set(\"name\", name)"
